   "source": [
    "import sympy as sp\n",
    "from anlis.polynomial import isContinuous\n",
    "\n",
    "x = sp.Symbol(\"x\")\n",
    "f1 = 1 / (x - 2)\n",
    "f2 = 2*x + x**(2/3)\n",
    "f3 = sp.exp(x) / (sp.exp(x) - 1)\n",
    "\n",
    "interval = (-1, 1)\n",
    "\n",
    "isContinuous(f1, interval), isContinuous(f2, interval), isContinuous(f3, interval)"
   ]
  }
